\ProvidesPackageRCS

\mode<presentation>

\definecolor{gtgold}{html}{b09465}
\definecolor{gtnavy}{html}{00223e}
\definecolor{gtwhite}{html}{ffffff}
\definecolor{gtltgray}{rgb}{0.97,0.97,0.97}

\setbeamercolor{section in toc}{fg=black,bg=white}
\setbeamercolor{alerted text}{fg=gtgold,bg=gtgold}
\setbeamercolor*{palette primary}{fg=gtgold,bg=gtnavy}
\setbeamercolor*{palette secondary}{fg=gtnavy,bg=gtgold}
\setbeamercolor*{palette tertiary}{fg=black,bg=gtwhite}
\setbeamercolor*{palette quaternary}{fg=gtnavy,bg=gtgold}

\setbeamercolor*{sidebar}{fg=gtgold,bg=gray}

\setbeamercolor*{palette sidebar primary}{fg=gtgold}
\setbeamercolor*{palette sidebar secondary}{fg=gtwhite}
\setbeamercolor*{palette sidebar tertiary}{fg=gtnavy,bg=gtgold}
\setbeamercolor*{palette sidebar quaternary}{fg=gtgold,bg=gtnavy}

\setbeamercolor{titlelike}{fg=gtnavy,bg=gtgold}
\setbeamercolor{frametitle}{fg=gtnavy,bg=gtgold}
\setbeamercolor{frametitle right}{bg=gtgold}

\setbeamercolor{block title}{fg=gtwhite,bg=gtgold}
\setbeamercolor{block body}{bg=gtltgray,fg=black}

\colorlet{ProgressColor}{gtwhite}
\colorlet{ProgressBack}{gtwhite}
\colorlet{ProgressBorder}{gtwhite}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>
